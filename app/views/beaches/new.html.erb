<h2>New Beach Form</h2>

<%= form_for(@beach) do |f| %>
    <div class="name">
        <%= f.label :name, "name" %> <br/>
        <%= f.text_field :name %>
    </div>

    <div class="town">
        <%= f.label :town, "town" %> <br/>
        <%= f.text_field :town %>
    </div>

    <div class="state">
        <%= f.label :state, "state" %> <br/>
        <%= f.text_field :state %>
    </div>

    <div class="description">
        <%= f.label :description, "description"  %> <br/>
        <%= f.text_field :description %>
    </div>

     <div class="url">
        <%= f.label :url %> <br/>
        <%= f.text_field :url %>
    </div>

     <div class="image">
        <%= f.label :image %> <br/>
        <%= f.text_field :image %>
    </div>

    <div class="action">
        <%= f.submit "Add Beach" %>
    </div>
<% end %>